&lt;h3 id=&#34;add-a-ceremony&#34;&gt;Add a ceremony&lt;/h3&gt;
&lt;div&gt;
    
    &lt;strong&gt;Copy the exact handwritten information from the ceremony papersheet&lt;/strong&gt;
    &lt;form id=&#34;ceremony-form&#34;&gt;

	  &lt;fieldset&gt;
        &lt;legend&gt;Date and local time of the ceremony&lt;/legend&gt;
        &lt;input type=&#34;datetime-local&#34; id=&#34;datetime&#34; name=&#34;datetime&#34; required&gt;
	  &lt;/fieldset&gt;

	  &lt;fieldset&gt;
        &lt;legend&gt;Identifier of the organizer of the ceremony&lt;/legend&gt;
        &lt;input type=&#34;text&#34; id=&#34;organizer_identifier&#34; name=&#34;organizer_identifier&#34; required&gt;
	  &lt;/fieldset&gt;

	  &lt;fieldset&gt;
        &lt;legend&gt;Location of the ceremony&lt;/legend&gt;
	    &lt;label&gt;Latitude:
          &lt;input type=&#34;number&#34; step=&#34;0.0001&#34; id=&#34;latitude&#34; name=&#34;latitude&#34; required&gt;
		&lt;/label&gt;
	    &lt;label&gt;Longitude:
          &lt;input type=&#34;number&#34; step=&#34;0.0001&#34; id=&#34;longitude&#34; name=&#34;longitude&#34; required&gt;
		&lt;/label&gt;
	  &lt;/fieldset&gt;

	  &lt;fieldset&gt;
	    &lt;legend&gt;Ethereum block of the ceremony&lt;/legend&gt;
	    &lt;label&gt;Block number:
          &lt;input type=&#34;number&#34; id=&#34;block_height&#34; name=&#34;block_height&#34; required&gt;
		&lt;/label&gt;
	    &lt;label&gt;Block hash:
          &lt;input type=&#34;text&#34; id=&#34;block_hash&#34; name=&#34;block_hash&#34; value=&#34;0x&#34; required&gt;
		&lt;/label&gt;
	  &lt;/fieldset&gt;

	  &lt;fieldset&gt;
	    &lt;legend&gt;Pictures and videos from the ceremony&lt;/legend&gt;
	    &lt;label&gt;Pictures:
          &lt;input type=&#34;file&#34; id=&#34;pictures&#34; name=&#34;pictures&#34; accept=&#34;image/*&#34; multiple required /&gt;
		&lt;/label&gt;
        &lt;br /&gt;
	    &lt;label&gt;Videos:
          &lt;input type=&#34;file&#34; id=&#34;videos&#34; name=&#34;videos&#34; accept=&#34;video/*&#34; multiple required /&gt;
		&lt;/label&gt;
	  &lt;/fieldset&gt;

	  &lt;fieldset&gt;
	    &lt;legend&gt;Identifiers and number of visible persons they identify&lt;/legend&gt;
	    &lt;table&gt;
		 &lt;thead&gt;
		  &lt;tr&gt;
		   &lt;th&gt;Phone number&lt;br /&gt; or email address&lt;/th&gt;
		   &lt;th&gt;Number of visible persons&lt;/th&gt;
		   &lt;th&gt;&lt;/th&gt;
		  &lt;/tr&gt;
		 &lt;/thead&gt;
		 &lt;tbody id=&#34;identifiers&#34;&gt;
		  &lt;tr&gt;
		   &lt;td&gt;&lt;input type=&#34;text&#34; name=&#34;identifier&#34; required /&gt;&lt;/td&gt;
		   &lt;td&gt;&lt;input type=&#34;number&#34; name=&#34;persons&#34; value=&#34;1&#34; required /&gt;&lt;/td&gt;
		   &lt;td&gt;&lt;input type=&#34;button&#34; id=&#34;del-row&#34; value=&#34;Remove&#34; hidden&gt;&lt;/td&gt;
		  &lt;/tr&gt;
		 &lt;/tbody&gt;
		 &lt;tfoot&gt;
		  &lt;tr&gt;
		   &lt;td colspan=&#34;3&#34;&gt;&lt;input type=&#34;button&#34; id=&#34;add-row&#34; name=&#34;add-row&#34; value=&#34;Add an identifier&#34; /&gt;&lt;/td&gt;
          &lt;/tr&gt;
		 &lt;/tfoot&gt;
		&lt;/table&gt;
	  &lt;/fieldset&gt;

